$("#js-form").empty();

<% if @question.errors.empty? %>
  $("tbody").append("<%= escape_javascript(render partial: 'question', locals: {question: @question}) %>")
<%- else %>
  $("#js-form").html("<%= escape_javascript(render partial: 'js_form', locals: {question: @question}) %>");
  new Rapidfire.QuestionForm($("#js-form #ja-question-form"));
<% end %>
